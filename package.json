{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:safe": "next dev",
    "build": "next build --turbo",
    "build:webpack": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:host": "next dev --turbo -p 12000 -H 0.0.0.0",
    "test:supabase": "node test-supabase.js",
    "test:chat": "node test-chat-api.js",
    "seed": "tsx scripts/seed-database.ts",
    "seed:clear": "tsx scripts/seed-database.ts --clear-only",
    "seed:test": "node scripts/seed-test-data.js",
    "seed:test-cleanup": "node scripts/seed-test-data.js cleanup",
    "migrate:projects": "node scripts/migrate-projects-to-client-companies.js",
    "migrate:projects-rollback": "node scripts/migrate-projects-to-client-companies.js rollback",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration", 
    "test:components": "jest --testPathPatterns=components",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:components"
  },
  "dependencies": {
    "@react-email/components": "^0.3.2",
    "@react-email/render": "^1.1.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.3",
    "@vercel/analytics": "^1.5.0",
    "dotenv": "^17.2.0",
    "next": "15.3.5",
    "nodemailer": "^7.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^4.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@google-cloud/local-auth": "^3.0.1",
    "@jest/globals": "^30.0.5",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
